<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.1//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.module.appframework">

	<class name="AppEnabled" table="appframework_app_enabled" batch-size="25">
		
		<cache usage="read-write"/>

		<id name="appEnabledId" type="java.lang.Integer" column="app_enabled_id" unsaved-value="undefined">
			<generator class="native">
				<param name="sequence">appframework_app_enabled_id_seq</param>
			</generator>
		</id>
	
		<property name="uuid" type="java.lang.String"
			column="uuid" length="38" unique="true" not-null="true"/>
			
		<property name="appName" type="java.lang.String" not-null="true" column="app_name" length="100" />
			
		<many-to-one name="user" class="org.openmrs.User" not-null="false" column="user"/>
		
		<many-to-one name="role" class="org.openmrs.Role" not-null="false" column="role"/>
		
	</class>

</hibernate-mapping>
